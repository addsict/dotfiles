[user]
    name = Yuki Furuyama
    email = addsict@gmail.com
[color]
    ui = auto
[core]
    excludesfile = $HOME/.gitignore
    editor = vim
[alias]
    st = "status --short --branch"
    co = checkout
    ci = commit
    br = branch
    lg = "log --graph --all --color --decorate --oneline -20"
    diff = "diff --histogram"
[pager]
    log = diff-highlight | less
    show = diff-highlight | less
    diff = diff-highlight | less
[include]
    path = ~/.gitconfig.local
[credential "https://source.developers.google.com"]
	helper = gcloud.sh
[pull]
	ff = only
